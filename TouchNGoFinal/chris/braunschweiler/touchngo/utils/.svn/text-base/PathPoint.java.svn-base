package chris.braunschweiler.touchngo.utils;

/**
 * Represents a point in 2D space. This class is used to sum up points in a path along which a sprite can move.
 * @author chrisbraunschweiler
 *
 */
public class PathPoint {
	private int xCoord;
	private int yCoord;
	
	public PathPoint(int xCoord, int yCoord){
		this.xCoord = xCoord;
		this.yCoord = yCoord;
	}
	
	public int getXCoord(){
		return this.xCoord;
	}
	
	public int getYCoord(){
		return this.yCoord;
	}
}
