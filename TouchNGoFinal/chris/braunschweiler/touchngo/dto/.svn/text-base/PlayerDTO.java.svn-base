package chris.braunschweiler.touchngo.dto;

import java.util.List;

import chris.braunschweiler.touchngo.common.CommonDataManager.PlayerAbility;
import chris.braunschweiler.touchngo.utils.PathPoint;

public class PlayerDTO {
	private int xPos;
	private int yPos;
	private PlayerAbility playerAbility;
	
	public PlayerDTO(int xPos, int yPos, PlayerAbility playerAbility){
		this.xPos = xPos;
		this.yPos = yPos;
		this.playerAbility = playerAbility;
	}
	
	public int getXPos(){
		return this.xPos;
	}
	
	public int getYPos(){
		return this.yPos;
	}

	public PlayerAbility getPlayerAbility() {
		return this.playerAbility;
	}
}
